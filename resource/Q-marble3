int OX_A()
{
	int x = 20;
	int y = 35;
	gotoxy(20, 35); printf("▶O◀");
	gotoxy(80, 35); printf("  X  ");
	while (1)
	{
		int n = keyControl();
		switch (n)
		{
		case RIGHT:
		{
			if (x < 80)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 3, y);
				printf("  ");
				for (int i = 0; i < 60; i++)
				{
					x++;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 3, y);
				printf("◀");
			}
			break;
		}
		case LEFT:
		{
			if (x > 20)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 3, y);
				printf("  ");
				for (int i = 0; i < 60; i++)
				{
					x--;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 3, y);
				printf("◀");
			}
			break;
		}
		case SELECT:
		{
			return x - 20;
		}
		}
	}
}
int Five_A()
{
	int x = 15;
	int y = 35;
	gotoxy(15, 35); printf("▶1◀");
	gotoxy(35, 35); printf("  2  ");
	gotoxy(55, 35); printf("  3  ");
	gotoxy(75, 35); printf("  4  ");
	gotoxy(95, 35); printf("  5  ");
	while (1)
	{
		int n = keyControl();
		switch (n)
		{
		case RIGHT:
		{
			if (x < 95)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 3, y);
				printf("  ");
				for (int i = 0; i < 20; i++)
				{
					x++;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 3, y);
				printf("◀");
			}
			break;
		}
		case LEFT:
		{
			if (x > 15)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 3, y);
				printf("  ");
				for (int i = 0; i < 20; i++)
				{
					x--;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 3, y);
				printf("◀");
			}
			break;
		}
		case SELECT:
		{
			return x;
		}
		}
	}
}
int Open_ended_A()
{
	gotoxy(50, 35); printf("정답 : ");
	gotoxy(58, 35);
}
int start_zone_bonus()
{
	gotoxy(32, 12); printf("★☆★☆★☆★☆★☆★☆★☆★☆★☆★☆");
	gotoxy(32, 13); printf("☆                                    ★");
	gotoxy(32, 14); printf("★           스타트 존 보너스         ☆");
	gotoxy(32, 15); printf("☆                                    ★");
	gotoxy(32, 16); printf("★               +20000원             ☆");
	gotoxy(32, 17); printf("☆                                    ★");
	gotoxy(32, 18); printf("★☆★☆★☆★☆★☆★☆★☆★☆★☆★☆");
	Sleep(500);
	gotoxy(32, 12); printf("☆★☆★☆★☆★☆★☆★☆★☆★☆★☆★");
	gotoxy(32, 13); printf("★                                    ☆");
	gotoxy(32, 14); printf("☆           스타트 존 보너스         ★");
	gotoxy(32, 15); printf("★                                    ☆");
	gotoxy(32, 16); printf("☆               +20000원             ★");
	gotoxy(32, 17); printf("★                                    ☆");
	gotoxy(32, 18); printf("☆★☆★☆★☆★☆★☆★☆★☆★☆★☆★");
	Sleep(500);
	gotoxy(32, 12); printf("★☆★☆★☆★☆★☆★☆★☆★☆★☆★☆");
	gotoxy(32, 13); printf("☆                                    ★");
	gotoxy(32, 14); printf("★           스타트 존 보너스         ☆");
	gotoxy(32, 15); printf("☆                                    ★");
	gotoxy(32, 16); printf("★               +20000원             ☆");
	gotoxy(32, 17); printf("☆                                    ★");
	gotoxy(32, 18); printf("★☆★☆★☆★☆★☆★☆★☆★☆★☆★☆");
	Sleep(500);
	gotoxy(32, 12); printf("☆★☆★☆★☆★☆★☆★☆★☆★☆★☆★");
