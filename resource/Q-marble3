#include <stdio.h>
#include <windows.h>
#include <stdlib.h>
#include <time.h>

/* 키보드 값 */
#define UP 72
#define DOWN 80
#define LEFT 75
#define RIGHT 77
#define SELECT 13
#define ESC 27
#define EASY 1
#define NOMOAL 2
#define HARD 3
#define EASY_MONEY 5000
#define NORMAL_MONEY 10000
#define HARD_MONEY 20000
#define PQ_MONEY 30000
#define START_ZONE_MONEY 20000

/* 함수 선언 */
