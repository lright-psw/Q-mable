void China()
{
	system("cls");
	setcolor(7, 0);
	printf("\n\n");
	printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                        ※ 중국 ※                                            ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     중국은 동아시아와 중앙아시아, 그리고 태평양 서부 연안의 도서 지방을 포괄하는 지명이다.   ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     우리가 자주 말하는 중국은 중화인민공화국을 뜻하는 떄가 많지만 사실 중화민국이라는 곳도   ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     있다.(국가라 하지 않은 이유 : UN에서 공식적으로 국가라 인정하지 않았다.)                 ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     일단 중국(중화인민공화국)은 수도는 베이징이고 주요 도시로는 베이징, 상하이, 광저우,      ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     난징, 시안, 푸저우, 칭다오, 톈진, 옌타이, 항저우 등이 있다. 인구수는 1,374,370,000명으로 ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     전세계 인구수 1위이다. 면적 순으로는 4위이다. 한국과 마찮가지로 일본과는 영토분쟁 중     ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     으로 사이가 좋지않다.  관광으로는 황허강에서 시작된 황허문명이 유명하다.                 ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■     p.s. 중국에서 가장 긴강이 황허라고 알고 있는 사람이 있지만 사실 양쯔강이 중국에서 가장   ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■          길고 세계에서 3번째로 길다.                                                         ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
}
int program_q()
{
	srand(time(NULL));
	int random = (rand() % 12) + 1;

	system("cls");
	setcolor(7, 0);
	if (random == 1)
	{
		char s1[20];
		char s2[20] = "세미콜론";
		printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                         난이도 ☆            ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                문제 : C 언어는 구문이 끝날 때 ____을 붙입니다.                               ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                         ____의 들어갈 말은?                                                  ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                   (발음을 한국어로 적어주세요. 맞춤법 틀리면 오답입니다.)                    ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                   ※  정답이라고 생각하는 답를 적고 엔터를 눌러주세요.                       ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
		Open_ended_A();
		scanf("%s", s1);
		if (strcmp(s1, s2) == 0)
		{
			return 0;
		}
		else
		{
			return 1;
		}
	}
	else if (random == 2)
	{
		char s1[20];
