void Country(int ctnum)
{
	if (ctnum == 1)
	{
		PH();
		CountDown();
	}
	else if (ctnum == 2)
	{
		Vietnam();
		CountDown();
	}
	else if (ctnum == 4)
	{
		India();
		CountDown();
	}
	else if (ctnum == 5)
	{
		Thailand();
		CountDown();
	}
	else if (ctnum == 6)
	{
		China();
		CountDown();
	}
	else if (ctnum == 7)
	{
		Japan();
		CountDown();
	}
	else if (ctnum == 9)
	{
		Turkey();
		CountDown();
	}
	else if (ctnum == 10)
	{
		France();
		CountDown();
	}
	else if (ctnum == 12)
	{
		Germany();
		CountDown();
	}
	else if (ctnum == 13)
	{
		Australia();
		CountDown();
	}
	else if (ctnum == 14)
	{
		USA();
		CountDown();
	}
	else if (ctnum == 15)
	{
		Korea();
		CountDown();
	}
}
void drawMap()
{
	int h, w;
	setcolor(7, 0);
	for (h = 0; h < 28; h++)
	{
		for (w = 0; w < 103; w++)
		{
			char temp = map[h][w];
			if (temp == '0')
			{
				printf(" ");
			}
			else if (temp == '1')
			{
				printf("┃");
			}
			else if (temp == '2')
			{
				printf("━");
			}
			else if (temp == '3')
			{
				printf("┏");
			}
			else if (temp == '4')
			{
				printf("┓");
			}
			else if (temp == '5')
			{
				printf("┛");
			}
			else if (temp == '6')
			{
				printf("┗");
			}
			else if (temp == '7')
			{
				printf("╋");
			}
			else if (temp == 'a')
			{
				printf("┣");
			}
			else if (temp == 'b')
			{
				printf("┳");
			}
			else if (temp == 'c')
			{
				printf("┫");
			}
			else if (temp == 'd')
			{
				printf("┻");
			}
		}
		printf("\n");
	}
	gotoxy(12, 22); printf("START");
	gotoxy(12, 26); printf("ZONE");
	//나라
	gotoxy(19, 17); printf("필");
	gotoxy(19, 18); printf("리");
	gotoxy(19, 19); printf("핀");
	//필리핀
	gotoxy(19, 11); printf("베");
	gotoxy(19, 12); printf("트");
	gotoxy(19, 13); printf("남");
	//베트남
	gotoxy(28, 8); printf("인 도");
	//인도
	gotoxy(44, 8); printf("태 국");
	//태국
	gotoxy(60, 8); printf("중 국");
	//중국
	gotoxy(76, 8); printf("일 본");
	//일본
	gotoxy(87, 11); printf("터");
	gotoxy(87, 13); printf("키");
	//터키
	gotoxy(87, 17); printf("프");
	gotoxy(87, 18); printf("랑");
	gotoxy(87, 19); printf("스");
	//프랑스
	gotoxy(75, 22); printf("독 일");
	//독일
	gotoxy(59, 22); printf("호 주");
	//호주
	gotoxy(43, 22); printf("미 국");
	//미국
	gotoxy(27, 22); printf("한 국");
	//한국
	gotoxy(8, 4); printf("C   언   어");
	gotoxy(8, 8); printf("퀴       즈");
	gotoxy(88, 4); printf("C   언   어");
	gotoxy(88, 8); printf("퀴       즈");
	gotoxy(88, 22); printf("C   언   어");
	gotoxy(88, 26); printf("퀴       즈");
	gotoxy(7, 2); printf("p1 : %d원", p1money);
	gotoxy(87, 2); printf("p2 : %d원", p2money);
	gotoxy(0, 30); printf("______________________________________________________________________________________________________________");

}
int keyControl()
{
	char temp = getch();

	if (temp == '↑')
	{
		return UP;
	}
	else if (temp == '←')
	{
		return LEFT;
	}
	else if (temp == '↓')
	{
		return DOWN;
	}
	else if (temp == '→')
	{
		return RIGHT;
	}
	else if (temp == '\n')
	{
		return SELECT;
	}
}
void init()
{
	system("mode con cols=110 lines=40");

	HANDLE consoleHandle = GetStdHandle(STD_OUTPUT_HANDLE);
	CONSOLE_CURSOR_INFO ConsoleCursor;
	ConsoleCursor.bVisible = 0;
	ConsoleCursor.dwSize = 1;
	SetConsoleCursorInfo(consoleHandle, &ConsoleCursor);
}
void titleDraw()
{
	printf("\n\n\n\n");
	printf("       QQQQQQQQQ \n");
	printf("     QQ:::::::::QQ \n");
	printf("   QQ:::::::::::::QQ \n");
	printf("  Q:::::::QQQ:::::::Q \n");
	printf("  Q::::::O   Q::::::Q \n");
	printf("  Q:::::O     Q:::::Q                  ::::    ::::      :::     :::::::::  :::::::::  :::        ::::::::::  \n");
	printf("  Q:::::O     Q:::::Q                  +:+:+: :+:+:+   :+: :+:   :+:    :+: :+:    :+: :+:        :+:         \n");
	printf("  Q:::::O     Q:::::Q                  +:+ +:+:+ +:+  +:+   +:+  +:+    +:+ +:+    +:+ +:+        +:+         \n");
	printf("  Q:::::O     Q:::::Q  +#++:++#++:++   +#+  +:+  +#+ +#++:++#++: +#++:++#:  +#++:++#+  +#+        +#++:++#    \n");
	printf("  Q:::::O     Q:::::Q                  +#+       +#+ +#+     +#+ +#+    +#+ +#+    +#+ +#+        +#+         \n");
	printf("  Q:::::O  QQQQ:::::Q                  #+#       #+# #+#     #+# #+#    #+# #+#    #+# #+#        #+#         \n");
	printf("  Q::::::O Q::::::::Q                  ###       ### ###     ### ###    ### #########  ########## ##########  \n");
	printf("  Q:::::::QQ::::::::Q \n");
	printf("   QQ::::::::::::::Q \n");
	printf("     QQ:::::::::::Q  \n");
	printf("       QQQQQQQQ::::QQ \n");
	printf("               Q:::::Q \n");
	printf("                QQQQQQ \n");
	printf("\n\n\n\n");
}
int menuDraw()
{
	int x = 50;
	int y = 27;
	gotoxy(x - 2, y);
	printf("> 게 임 시 작");
	gotoxy(x, y + 1);
	printf("게 임 정 보");
	gotoxy(x, y + 2);
	printf("   종 료   ");

	while (1)
	{
		int n = keyControl();
		switch (n)
		{
		case UP:
		{
			if (y > 27)
			{
				gotoxy(x - 2, y);
				printf(" ");
				gotoxy(x - 2, --y);
				printf(">");
			}
			break;
		}
		case DOWN:
		{
			if (y < 29)
			{
				gotoxy(x - 2, y);
				printf(" ");
				gotoxy(x - 2, ++y);
				printf(">");
			}
			break;
		}
		case SELECT:
		{
			return y - 27;
		}
		}
	}
}
void infoDraw()
{
	system("cls");
	printf("\n\n\n");
	printf("	  ■■■■■■■■■■■■■■■■■Q-marble 규칙 설명■■■■■■■■■■■■■■■■■■ \n");
	printf("	  ■                                                                                    ■ \n");
	printf("	  ■ 1. 주사위를 던져서 나온 수(1~3) 만큼 칸을 이동한다.                                ■ \n");
	printf("	  ■ 2. 각 칸에서 퀴즈를 풀어야 한다.                                                   ■ \n");
	printf("	  ■          -나라 퀴즈 칸: 12개                                                       ■ \n");
	printf("	  ■          -프로그래밍 퀴즈 칸: 3개                                                  ■ \n");
	printf("	  ■ 3. 나라 퀴즈 칸에 도착하면 상중하 난이도를 선택한다.                               ■ \n");
	printf("	  ■          - 문제를 맞췄을 시                                                        ■ \n");
	printf("	  ■            상 : +20000                                                             ■ \n");
	printf("	  ■            중 : +10000                                                             ■ \n");
	printf("	  ■            하 : +5000                                                              ■ \n");
	printf("	  ■          - 문제를 틀렸을 시                                                        ■ \n");
	printf("	  ■            상 : -20000                                                             ■ \n");
	printf("	  ■            중 : -10000                                                             ■ \n");
	printf("	  ■            하 : -5000                                                              ■ \n");
	printf("	  ■ 4. 프로그래밍 퀴즈 칸에 도착하면 프로그래밍 문제 12개 중 랜덤으로 문제가 주어진다. ■ \n");
	printf("	  ■          - 문제를 맞췄을 시  +30000                                                ■ \n");
	printf("	  ■          - 문제를 틀렸을 시  -30000                                                ■ \n");
	printf("	  ■ 5. 시작 칸에 도착할 시 +20000이 부여된다.                                          ■ \n");
	printf("	  ■ 6. 게임 엔딩 조건                                                                  ■ \n");
	printf("	  ■          - 한 명이 파산하거나 한 명이 먼저 250,000을 넘기면 승리하게 된다.         ■ \n");
	printf("	  ■                                                                                    ■ \n");
	printf("	  ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■ \n");
	printf("\n\n\n");
	printf("				*시작 화면으로 돌아가실려면 ESC키를 눌러주세요*");

	while (1)
	{
		if (keyControl() == ESC)
		{
			break;
		}
	}
}

void gotoxy(int x, int y)
{
	HANDLE consoleHandle = GetStdHandle(STD_OUTPUT_HANDLE);
	COORD pos;
	pos.X = x;
	pos.Y = y;
	SetConsoleCursorPosition(consoleHandle, pos);
}
void setcolor(unsigned short text, unsigned short back)
{
	SetConsoleTextAttribute(GetStdHandle(STD_OUTPUT_HANDLE), text | (back << 4));
}
