void p1bin()
{
	gotoxy(player1_x, player1_y); printf("     ");
	gotoxy(player1_x, player1_y + 1); printf("     ");
	gotoxy(player1_x, player1_y + 2); printf("     ");
}
void p2bin()
{
	gotoxy(player2_x, player2_y); printf("     ");
	gotoxy(player2_x, player2_y + 1); printf("     ");
	gotoxy(player2_x, player2_y + 2); printf("     ");
}
void bin()
{
	Sleep(50);
	gotoxy(dice_x, dice_y); printf("                       \n");
	gotoxy(dice_x, dice_y + 1); printf("                       \n");
	gotoxy(dice_x, dice_y + 2); printf("                       \n");
	gotoxy(dice_x, dice_y + 3); printf("                       \n");
	gotoxy(dice_x, dice_y + 4); printf("                       \n");
	gotoxy(dice_x, dice_y + 5); printf("                       \n");
	gotoxy(dice_x, dice_y + 6); printf("                         ");
}
void p1go()
{
	if (random == 1)
	{
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
	}
	else if (random == 2)
	{
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
	}
	else if (random == 3)
	{
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
	}
}
void p2go()
{
	if (random == 1)
	{
		dice2 = (dice2 + 1) % 16;
		p2move();
	}
	else if (random == 2)
	{
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
	}
	else if (random == 3)
	{
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
	}
}
int LevelChoose()
{
	int x = 20;
	int y = 25;
	setcolor(7, 0);
	system("cls");
	gotoxy(30, 8);  printf(" __       _______ ____    ____  _______  __             ");
	gotoxy(30, 9);  printf("|  |     |   ____|\\   \\  /   / |   ____||  |           ");
	gotoxy(30, 10); printf("|  |     |  |__    \\   \\/   /  |  |__   |  |         ");
	gotoxy(30, 11); printf("|  |     |   __|    \\      /   |   __|  |  |       ");
	gotoxy(30, 12); printf("|  `----.|  |____    \\    /    |  |____ |  `----.     ");
	gotoxy(30, 13); printf("|_______||_______|    \\__/     |_______||_______|     ");
	gotoxy(21, 15);  printf("   ______  __    __    ______     ______        _______. _______ ");
	gotoxy(21, 16);  printf("  /      ||  |  |  |  /  __  \\   /  __  \\      /       ||   ____|");
	gotoxy(21, 17); printf(" |  ,----'|  |__|  | |  |  |  | |  |  |  |    |   (----`|  |__   ");
	gotoxy(21, 18); printf(" |  |     |   __   | |  |  |  | |  |  |  |     \\   \\    |   __|  ");
	gotoxy(21, 19); printf(" |  `----.|  |  |  | |  `--'  | |  `--'  | .----)   |   |  |____ ");
	gotoxy(21, 20); printf("  \\______||__|  |__|  \\______/   \\______/  |_______/    |_______|");
	gotoxy(40, 22); printf("문제 난이도를 선택하세요.");
	gotoxy(20, 25); printf("▶하◀");
	gotoxy(50, 25); printf("  중  ");
	gotoxy(80, 25); printf("  상  ");
	while (1)
	{
		int n = keyControl();
		switch (n)
		{
		case RIGHT:
		{
			if (x < 80)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 4, y);
				printf("  ");
				for (int i = 0; i < 30; i++)
				{
					x++;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 4, y);
				printf("◀");
			}
			break;
		}
		case LEFT:
		{
			if (x > 20)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 4, y);
				printf("  ");
				for (int i = 0; i < 30; i++)
				{
					x--;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 4, y);
				printf("◀");
			}
			break;
		}
		case SELECT:
		{
			return x - 20;
		}
		}
	}
}
void CountDown()
{
	for (int i = 5; i >= 1; i--)
	{
		gotoxy(30, 35); printf("%d초뒤 화면이 넘어갑니다.", i);
		Sleep(1000);
		gotoxy(30, 35); printf("                         ");
	}
}
void France()
{
	system("cls");
	setcolor(7, 0);
	printf("\n\n");
	printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                               ※        프랑스        ※                                     ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      프랑스는 자유로운이라는 뜻의 라틴어인 Francia로부터 유래되었다.서유럽의 본토와          ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      남아메리카의 프랑스령 기아나를 비롯해 여러 대륙에 걸쳐 있는 해외 레지옹(행정구역)과     ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      해외 영토로 이루어진 국가로서, 유럽 연합 소속 국가 중 영토가 가장 넓다.                 ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      수도는 파리이며, 프랑스 본토는 남북으로는 지중해에서 영국 해협과 북해까지,              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      동서로는 라인강에서 대서양에 이른다                                                     ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      현재는 세계에서 손꼽히는 선진국이며 2018년 현재 명목 국내총생산은 세계에서 여섯 번째,   ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      유럽에서 세 번째로 크다.  프랑스의 대표적인 사건으로는 프랑스 혁명이 있고               ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■      또, 유명한 랜드마크로는 에펠탑이 있다.                                                  ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■                                                                                              ■\n");
	printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
}
