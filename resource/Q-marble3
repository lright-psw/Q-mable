	gotoxy(dice_x, dice_y + 5); printf("┃ ●        ┃┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━━━━━━━━━━━┻┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎━━━━━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃        ●  ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃            ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃     ●     ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃            ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃  ●        ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━━━━━━━━━━━━┛ ");
}
void player1()
{
	setcolor(9, 0);
	gotoxy(player1_x, player1_y);     printf("  1 ");
	gotoxy(player1_x, player1_y + 1); printf(" /|\\");
	gotoxy(player1_x, player1_y + 2); printf(" / \\ ");
}
void player2()
{
	setcolor(12, 0);
	gotoxy(player2_x, player2_y);     printf("  2 ");
	gotoxy(player2_x, player2_y + 1); printf(" /|\\");
	gotoxy(player2_x, player2_y + 2); printf(" / \\ ");
}
void p1move()
{
	switch (dice1)
	{
	case 0:
		player1_x = 31;
		player1_y = 23;
		p1bin();
		player1_x = 12;
		player1_y = 23;
		player1();
		break;
	case 1:
		player1_x = 12;
		player1_y = 23;
		p1bin();
		player1_x = 12;
		player1_y = 17;
		player1();
		break;
	case 2:
		player1_x = 12;
		player1_y = 17;
		p1bin();
		player1_x = 12;
		player1_y = 11;
		player1();
		break;
	case 3:
		player1_x = 12;
		player1_y = 11;
		p1bin();
		player1_x = 15;
		player1_y = 5;
		player1();
		break;
	case 4:
		player1_x = 15;
		player1_y = 5;
		p1bin();
		player1_x = 31;
		player1_y = 5;
		player1();
		break;
	case 5:
		player1_x = 31;
		player1_y = 5;
		p1bin();
		player1_x = 47;
		player1_y = 5;
		player1();
		break;
	case 6:
		player1_x = 47;
		player1_y = 5;
		p1bin();
		player1_x = 63;
		player1_y = 5;
		player1();
		break;
	case 7:
		player1_x = 63;
		player1_y = 5;
		p1bin();
		player1_x = 79;
		player1_y = 5;
		player1();
		break;
	case 8:
		player1_x = 79;
		player1_y = 5;
		p1bin();
		player1_x = 95;
		player1_y = 5;
		player1();
		break;
	case 9:
		player1_x = 95;
		player1_y = 5;
		p1bin();
		player1_x = 95;
		player1_y = 11;
		player1();
		break;
	case 10:
		player1_x = 95;
		player1_y = 11;
		p1bin();
		player1_x = 95;
		player1_y = 17;
		player1();
		break;
	case 11:
		player1_x = 95;
		player1_y = 17;
		p1bin();
		player1_x = 95;
		player1_y = 23;
		player1();
		break;
	case 12:
		player1_x = 95;
		player1_y = 23;
		p1bin();
		player1_x = 79;
		player1_y = 23;
		player1();
		break;
	case 13:
		player1_x = 79;
		player1_y = 23;
		p1bin();
		player1_x = 63;
		player1_y = 23;
		player1();
		break;
	case 14:
		player1_x = 63;
		player1_y = 23;
		p1bin();
		player1_x = 47;
		player1_y = 23;
		player1();
		break;
	case 15:
		player1_x = 47;
		player1_y = 23;
		p1bin();
		player1_x = 31;
		player1_y = 23;
		player1();
		break;
	}
}
