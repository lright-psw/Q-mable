int dice_x, dice_y;
int player1_x, player1_y, player2_x, player2_y;
int dice1, dice2;
int p1money = 100000, p2money = 100000;
int random;
int FAcode;
int OXcode;
char As[20];
int s = 0;
///////////////////////////////////////////////////////////////////////////
int main()
{
	init();
	while (1)
	{
		titleDraw();
		int menuCode = menuDraw();
		if (menuCode == 0)
		{
			system("cls");
			drawMap();
			player1_x = 12;
			player1_y = 23;
			player1();
			player2_x = 7;
			player2_y = 23;
			player2();
			setcolor(7, 0);
			while (1)
			{
				dice();
				p1go();
				Country(dice1);
				if (dice1 == 3 || dice1 == 8 || dice1 == 11)
				{
					int PQcode = program_q();
					if (PQcode == 0)
					{
						p1money = p1money + PQ_MONEY;
						system("cls");
						drawMap();
						p1move();
						p2move();
						correct();
					}
					else
					{
						p1money = p1money - PQ_MONEY;
						system("cls");
						drawMap();
						p1move();
						p2move();
						wrong();
					}
				}
				else if (dice1 == 0)
				{
					p1money = p1money + START_ZONE_MONEY;
					system("cls");
					drawMap();
					p1move();
					p2move();
					start_zone_bonus();
				}
				else
				{
					int levelcode = LevelChoose();
					if (levelcode == 0)
					{
						int ELcode = EasyLevel_Q(dice1);
						if (ELcode == 0)
						{
							p1money = p1money + EASY_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							correct();
						}
						else
						{
							p1money = p1money - EASY_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							wrong();
						}
					}
					else if (levelcode == 30)
					{
						int NLcode = NormalLevel_Q(dice1);
						if (NLcode == 0)
						{
							p1money = p1money + NORMAL_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							correct();
						}
						else
						{
							p1money = p1money - NORMAL_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							wrong();
						}
					}
					else if (levelcode == 60)
					{
						int HLcode = HardLevel_Q(dice1);
						if (HLcode == 0)
						{
							p1money = p1money + HARD_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							correct();
						}
						else
						{
							p1money = p1money - HARD_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							wrong();
						}
					}
				}
				if (p1money >= 250000)
				{
					setcolor(11, 0);
					gotoxy(32, 12); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(32, 13); printf("＃                                      ＃");
					gotoxy(32, 14); printf("＃      player1이 일정 금액을 넘겨      ＃");
					gotoxy(32, 15); printf("＃                                      ＃");
					gotoxy(32, 16); printf("＃            player1 승리!             ＃");
					gotoxy(32, 17); printf("＃                                      ＃");
					gotoxy(32, 18); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(0, 35); return 0;
				}
				if (p1money <= 0)
				{
					setcolor(11, 0);
					gotoxy(32, 12); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(32, 13); printf("＃                                      ＃");
					gotoxy(32, 14); printf("＃          player1이 파산하여          ＃");
					gotoxy(32, 15); printf("＃                                      ＃");
					gotoxy(32, 16); printf("＃            player2 승리!             ＃");
					gotoxy(32, 17); printf("＃                                      ＃");
					gotoxy(32, 18); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(0, 35); return 0;
				}
				dice();
				p2go();
				Country(dice2);
				if (dice2 == 3 || dice2 == 8 || dice2 == 11)
				{
					int PQcode = program_q();
					if (PQcode == 0)
					{
						p2money = p2money + PQ_MONEY;
						system("cls");
						drawMap();
						p1move();
						p2move();
						correct();
					}
					else
					{
						p2money = p2money - PQ_MONEY;
						system("cls");
						drawMap();
						p1move();
						p2move();
						wrong();
					}
				}
				else if (dice2 == 0)
				{
					p2money = p2money + START_ZONE_MONEY;
					system("cls");
					drawMap();
					p1move();
					p2move();
					start_zone_bonus();
				}
				else
				{
					int levelcode = LevelChoose();
					if (levelcode == 0)
					{
						int ELcode = EasyLevel_Q(dice2);
						if (ELcode == 0)
						{
							p2money = p2money + EASY_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							correct();
						}
						else
						{
							p2money = p2money - EASY_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							wrong();
						}
					}
					else if (levelcode == 30)
					{
						int NLcode = NormalLevel_Q(dice2);
						if (NLcode == 0)
						{
							p2money = p2money + NORMAL_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							correct();
						}
						else
						{
							p2money = p2money - NORMAL_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							wrong();
						}
					}
					else if (levelcode == 60)
					{
						int HLcode = HardLevel_Q(dice2);
						if (HLcode == 0)
						{
							p2money = p2money + HARD_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							correct();
						}
						else
						{
							p2money = p2money - HARD_MONEY;
							system("cls");
							drawMap();
							p1move();
							p2move();
							wrong();
						}
					}
				}
				if (p2money >= 250000)
				{
					setcolor(11, 0);
					gotoxy(32, 12); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(32, 13); printf("＃                                      ＃");
					gotoxy(32, 14); printf("＃      player2이 일정 금액을 넘겨      ＃");
					gotoxy(32, 15); printf("＃                                      ＃");
					gotoxy(32, 16); printf("＃            player2 승리!             ＃");
					gotoxy(32, 17); printf("＃                                      ＃");
					gotoxy(32, 18); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(0, 35); return 0;
				}
				if (p2money <= 0)
				{
					setcolor(11, 0);
					gotoxy(32, 12); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(32, 13); printf("＃                                      ＃");
					gotoxy(32, 14); printf("＃          player2이 파산하여          ＃");
					gotoxy(32, 15); printf("＃                                      ＃");
					gotoxy(32, 16); printf("＃            player1 승리!             ＃");
					gotoxy(32, 17); printf("＃                                      ＃");
					gotoxy(32, 18); printf("＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃");
					gotoxy(0, 35); return 0;
				}
			}
		}
		else if (menuCode == 1)
		{
			infoDraw();
		}
		else if (menuCode == 2)
		{
			return 0;
		}
		system("cls");
	}
	return 0;
}

/* 함수 구현 */
void dice()
{
	int key;
	int dice_x = 60, dice_y;
	int i = 0;
	do
	{
		setcolor(9, 0);
		gotoxy(20, 35);  printf("주사위를 굴려주세요");
		Sleep(500);
		setcolor(15, 0);
		gotoxy(20, 35);  printf("주사위를 굴려주세요");
		Sleep(500);
		if (kbhit())
		{
			gotoxy(20, 35); printf("                             ");
			break;
		}
	} while (1);

	while (1)
	{
		key = getch();
		if (key == SELECT)
		{
			srand(time(NULL));
			random = (rand() % 3) + 1;
			if (random == 1)
			{
				dicemove1();
				break;
			}
			else if (random == 2)
			{
				dicemove2();
				break;
			}
			else if (random == 3)
			{
				dicemove3();
				break;
			}
		}
	}
	Sleep(1000);
}
void dicemove1()
{
	dice_x = 30;
	dice_y = 12;
	gotoxy(dice_x, dice_y);     printf("┎━━━━━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃            ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃       ●   ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃            ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃   ●       ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃            ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━━━━━━━━━━━━┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎┳━━━━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃┃           ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃┃       ●  ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃┃           ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃┃   ●      ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃┃           ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖┻━━━━━━━━━━━┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎━┳━━━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃ ┃          ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃ ┃      ●  ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃ ┃          ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃ ┃  ●      ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃ ┃          ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━┻━━━━━━━━━━┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎━━┳━━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃●┃         ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃  ┃      ● ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃●┃         ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃  ┃  ●     ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃●┃         ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━━┻━━━━━━━━━┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎━━━┳━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃ ●┃        ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃   ┃     ● ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃ ●┃        ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃   ┃  ●    ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃● ┃        ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━━━┻━━━━━━━━┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎━━━━┳━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃  ●┃       ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃    ┃    ● ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃ ● ┃       ┃ \n");
	gotoxy(dice_x, dice_y + 4); printf("┃    ┃ ●    ┃ \n");
	gotoxy(dice_x, dice_y + 5); printf("┃●  ┃       ┃ \n");
	gotoxy(dice_x, dice_y + 6); printf("┖━━━━┻━━━━━━━┛ ");
	bin();
	dice_x++;
	gotoxy(dice_x, dice_y);     printf("┎━━━━━┳━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃  ● ┃      ┃ \n");
