		printf("    ■                                                                                              ■\n");
		printf("    ■                             ●                                                               ■\n");
		printf("    ■                             ●                                                               ■\n");
		printf("    ■                             ●                                                               ■\n");
		printf("    ■                   }                                                                          ■\n");
		printf("    ■               }                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                 (;은 색략하시오.)                                            ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                    ※  정답이라고 생각하는 답를 적고 엔터를 눌러주세요.                      ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■                                                                                              ■\n");
		printf("    ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
		scanf("%s", s1);
		if (strcmp(s1, s2) == 0)
		{
			correct();
		}
		else
		{
			wrong();
		}
	}
}
int start_zone_bonus()
{
	system("cls");
	printf("★☆★☆★☆★☆★☆★☆★☆★☆★☆");
	printf("☆                                ★");
	printf("★        스타트 존 보너스        ☆");
	printf("☆                                ★");
	printf("★            +20000원            ☆");
	printf("☆                                ★");
	printf("★☆★☆★☆★☆★☆★☆★☆★☆★☆");
}
void correct()
{
	system("cls");
