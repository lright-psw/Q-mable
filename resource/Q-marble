void player1()
{
	setcolor(9, 0);
	gotoxy(player1_x, player1_y);     printf("  1 ");
	gotoxy(player1_x, player1_y + 1); printf(" /|\\");
	gotoxy(player1_x, player1_y + 2); printf(" / \\ ");
}
void player2()
{
	setcolor(12, 0);
	gotoxy(player2_x, player2_y);     printf("  2 ");
	gotoxy(player2_x, player2_y + 1); printf(" /|\\");
	gotoxy(player2_x, player2_y + 2); printf(" / \\ ");
}
void p1move()
{
	switch (dice1)
	{
	case 0:
		player1_x = 31;
		player1_y = 23;
		p1bin();
		player1_x = 12;
		player1_y = 23;
		player1();
		break;
	case 1:
		player1_x = 12;
		player1_y = 23;
		p1bin();
		player1_x = 12;
		player1_y = 17;
		player1();
		break;
	case 2:
		player1_x = 12;
		player1_y = 17;
		p1bin();
		player1_x = 12;
		player1_y = 11;
		player1();
		break;
	case 3:
		player1_x = 12;
		player1_y = 11;
		p1bin();
		player1_x = 15;
		player1_y = 5;
		player1();
		break;
	case 4:
		player1_x = 15;
		player1_y = 5;
		p1bin();
		player1_x = 31;
		player1_y = 5;
		player1();
		break;
	case 5:
		player1_x = 31;
		player1_y = 5;
		p1bin();
		player1_x = 47;
		player1_y = 5;
		player1();
		break;
	case 6:
		player1_x = 47;
		player1_y = 5;
		p1bin();
		player1_x = 63;
		player1_y = 5;
		player1();
		break;
	case 7:
		player1_x = 63;
		player1_y = 5;
		p1bin();
		player1_x = 79;
		player1_y = 5;
		player1();
		break;
	case 8:
		player1_x = 79;
		player1_y = 5;
		p1bin();
		player1_x = 95;
		player1_y = 5;
		player1();
		break;
	case 9:
		player1_x = 95;
		player1_y = 5;
		p1bin();
		player1_x = 95;
		player1_y = 11;
		player1();
		break;
	case 10:
		player1_x = 95;
		player1_y = 11;
		p1bin();
		player1_x = 95;
		player1_y = 17;
		player1();
		break;
	case 11:
		player1_x = 95;
		player1_y = 17;
		p1bin();
		player1_x = 95;
		player1_y = 23;
		player1();
		break;
	case 12:
		player1_x = 95;
		player1_y = 23;
		p1bin();
		player1_x = 79;
		player1_y = 23;
		player1();
		break;
	case 13:
		player1_x = 79;
		player1_y = 23;
		p1bin();
		player1_x = 63;
		player1_y = 23;
		player1();
		break;
	case 14:
		player1_x = 63;
		player1_y = 23;
		p1bin();
		player1_x = 47;
		player1_y = 23;
		player1();
		break;
	case 15:
		player1_x = 47;
		player1_y = 23;
		p1bin();
		player1_x = 31;
		player1_y = 23;
		player1();
		break;
	}
}
void p2move()
{
	switch (dice2)
	{
	case 0:
		player2_x = 23;
		player2_y = 23;
		p2bin();
		player2_x = 7;
		player2_y = 23;
		player2();
		break;
	case 1:
		player2_x = 7;
		player2_y = 23;
		p2bin();
		player2_x = 7;
		player2_y = 17;
		player2();
		break;
	case 2:
		player2_x = 7;
		player2_y = 17;
		p2bin();
		player2_x = 7;
		player2_y = 11;
		player2();
		break;
	case 3:
		player2_x = 7;
		player2_y = 11;
		p2bin();
		player2_x = 7;
		player2_y = 5;
		player2();
		break;
	case 4:
		player2_x = 7;
		player2_y = 5;
		p2bin();
		player2_x = 23;
		player2_y = 5;
		player2();
		break;
	case 5:
		player2_x = 23;
		player2_y = 5;
		p2bin();
		player2_x = 39;
		player2_y = 5;
		player2();
		break;
	case 6:
		player2_x = 39;
		player2_y = 5;
		p2bin();
		player2_x = 55;
		player2_y = 5;
		player2();
		break;
	case 7:
		player2_x = 55;
		player2_y = 5;
		p2bin();
		player2_x = 71;
		player2_y = 5;
		player2();
		break;
	case 8:
		player2_x = 71;
		player2_y = 5;
		p2bin();
		player2_x = 87;
		player2_y = 5;
		player2();
		break;
	case 9:
		player2_x = 87;
		player2_y = 5;
		p2bin();
		player2_x = 90;
		player2_y = 11;
		player2();
		break;
	case 10:
		player2_x = 90;
		player2_y = 11;
		p2bin();
		player2_x = 90;
		player2_y = 17;
		player2();
		break;
	case 11:
		player2_x = 90;
		player2_y = 17;
		p2bin();
		player2_x = 87;
		player2_y = 23;
		player2();
		break;
	case 12:
		player2_x = 87;
		player2_y = 23;
		p2bin();
		player2_x = 71;
		player2_y = 23;
		player2();
		break;
	case 13:
		player2_x = 71;
		player2_y = 23;
		p2bin();
		player2_x = 55;
		player2_y = 23;
		player2();
		break;
	case 14:
		player2_x = 55;
		player2_y = 23;
		p2bin();
		player2_x = 39;
		player2_y = 23;
		player2();
		break;
	case 15:
		player2_x = 39;
		player2_y = 23;
		p2bin();
		player2_x = 23;
		player2_y = 23;
		player2();
		break;
	}
}
void p1bin()
{
	gotoxy(player1_x, player1_y); printf("     ");
	gotoxy(player1_x, player1_y + 1); printf("     ");
	gotoxy(player1_x, player1_y + 2); printf("     ");
}
void p2bin()
{
	gotoxy(player2_x, player2_y); printf("     ");
	gotoxy(player2_x, player2_y + 1); printf("     ");
	gotoxy(player2_x, player2_y + 2); printf("     ");
}
void bin()
{
	Sleep(50);
	gotoxy(dice_x, dice_y); printf("                       \n");
	gotoxy(dice_x, dice_y + 1); printf("                       \n");
	gotoxy(dice_x, dice_y + 2); printf("                       \n");
	gotoxy(dice_x, dice_y + 3); printf("                       \n");
	gotoxy(dice_x, dice_y + 4); printf("                       \n");
	gotoxy(dice_x, dice_y + 5); printf("                       \n");
	gotoxy(dice_x, dice_y + 6); printf("                         ");
}
void p1go()
{
	if (random == 1)
	{
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
	}
	else if (random == 2)
	{
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
	}
	else if (random == 3)
	{
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
		dice1 = (dice1 + 1) % 16;
		p1move();
		Sleep(500);
	}
}
void p2go()
{
	if (random == 1)
	{
		dice2 = (dice2 + 1) % 16;
		p2move();
	}
	else if (random == 2)
	{
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
	}
	else if (random == 3)
	{
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
		dice2 = (dice2 + 1) % 16;
		p2move();
		Sleep(500);
	}
}
int LevelChoose()
{
	int x = 20;
	int y = 25;
	setcolor(7, 0);
	system("cls");
	gotoxy(30, 8);  printf(" __       _______ ____    ____  _______  __             ");
	gotoxy(30, 9);  printf("|  |     |   ____|\\   \\  /   / |   ____||  |           ");
	gotoxy(30, 10); printf("|  |     |  |__    \\   \\/   /  |  |__   |  |         ");
	gotoxy(30, 11); printf("|  |     |   __|    \\      /   |   __|  |  |       ");
	gotoxy(30, 12); printf("|  `----.|  |____    \\    /    |  |____ |  `----.     ");
	gotoxy(30, 13); printf("|_______||_______|    \\__/     |_______||_______|     ");
	gotoxy(21, 15);  printf("   ______  __    __    ______     ______        _______. _______ ");
	gotoxy(21, 16);  printf("  /      ||  |  |  |  /  __  \\   /  __  \\      /       ||   ____|");
	gotoxy(21, 17); printf(" |  ,----'|  |__|  | |  |  |  | |  |  |  |    |   (----`|  |__   ");
	gotoxy(21, 18); printf(" |  |     |   __   | |  |  |  | |  |  |  |     \\   \\    |   __|  ");
	gotoxy(21, 19); printf(" |  `----.|  |  |  | |  `--'  | |  `--'  | .----)   |   |  |____ ");
	gotoxy(21, 20); printf("  \\______||__|  |__|  \\______/   \\______/  |_______/    |_______|");
	gotoxy(40, 22); printf("문제 난이도를 선택하세요.");
	gotoxy(20, 25); printf("▶하◀");
	gotoxy(50, 25); printf("  중  ");
	gotoxy(80, 25); printf("  상  ");
	while (1)
	{
		int n = keyControl();
		switch (n)
		{
		case RIGHT:
		{
			if (x < 80)
			{
				gotoxy(x, y);  
				printf("  ");
				for (int i = 0; i < 30; i++)
				{
					x++;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 4, y);
				printf("◀");
			}
			break;
		}
		case LEFT:
		{
			if (x > 20)
			{
				gotoxy(x, y);
				printf("  ");
				gotoxy(x + 4, y);
				printf("  ");
				for (int i = 0; i < 30; i++)
				{
					x--;
				}
				gotoxy(x, y);
				printf("▶");
				gotoxy(x + 4, y);
				printf("◀");
			}
			break;
		}
		case SELECT:
		{
			return x - 20;
		}
		}
	}
}
void CountDown()
{
	for (int i = 5; i >= 1; i--)
	{
		gotoxy(30, 35); printf("%d초뒤 화면이 넘어갑니다.", i);
		Sleep(1000);
		gotoxy(30, 35); printf("                         ");
	}
}
