///////////////////////////////////////////////////////////////////////////
int main()
{
	init();
	while (1)
	{
		titleDraw();
		int menuCode = menuDraw();
		if (menuCode == 0)
		{
			system("cls");
			drawMap();
			while (1)
			{
				dice();
				p1go();
				Country(dice1);
				Sleep(5000);
				dice();
				p2go();
			}
		}
		else if (menuCode == 1)
		{
			infoDraw();
		}
		else if (menuCode == 2)
		{
			return 0;
		}
		system("cls");
	}
	return 0;
}

/* 함수 구현 */
void dice()
{
	int key;
	int dice_x = 60, dice_y;
	int i = 0;
	do
	{
		setcolor(9, 0);
		gotoxy(20, 35);  printf("주사위를 굴려주세요");
		Sleep(500);
		setcolor(15, 0);
		gotoxy(20, 35);  printf("주사위를 굴려주세요");
		Sleep(500);
		if (kbhit())
		{
			gotoxy(20, 35); printf("                             ");
			break;
		}
	} while (1);

	while (1)
	{
		key = getch();
		if (key == SELECT)
		{
			srand(time(NULL));
			random = (rand() % 3) + 1;
			if (random == 1)
			{
				dicemove1();
				break;
			}
			else if (random == 2)
			{
				dicemove2();
				break;
			}
			else if (random == 3)
			{
				dicemove3();
				break;
			}
		}
	}
	Sleep(1000);
}
void dicemove1()
{
	dice_x = 30;
	dice_y = 12;
	gotoxy(dice_x, dice_y);     printf("┎━━━━━━━━━━━━┓ \n");
	gotoxy(dice_x, dice_y + 1); printf("┃            ┃ \n");
	gotoxy(dice_x, dice_y + 2); printf("┃       ●   ┃ \n");
	gotoxy(dice_x, dice_y + 3); printf("┃            ┃ \n");
